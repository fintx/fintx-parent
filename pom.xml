	<properties>
		<java.version>1.8</java.version>
		<!-- spring cloud -->
		<spring-cloud.version>Finchley.M1</spring-cloud.version>

		<!-- spring boot -->
		<spring-boot-admin.version>1.5.2</spring-boot-admin.version>
